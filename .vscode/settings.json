{
    "python.languageServer": "Jedi",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "python.formatting.provider": "none",
    "cSpell.words": [
        "asyncpg",
        "asyncpraw",
        "blurple",
        "Cooldown",
        "deletehi",
        "Dnlf",
        "fetchrow",
        "fetchval",
        "hazrat",
        "hazratummar",
        "ihazratummar",
        "imgur",
        "Mqenrf",
        "passw",
        "resetxp",
        "sethi",
        "stickied",
        "Tqrqc",
        "unban",
        "unmute",
        "userinfo"
    ],
    "intelephense.files.exclude": [
        "**/.git/**",
        "**/.svn/**",
        "**/.hg/**",
        "**/CVS/**",
        "**/.DS_Store/**",
        "**/node_modules/**",
        "**/bower_components/**",
        "**/vendor/**/{Tests,tests}/**",
        "**/.history/**",
        "**/vendor/**/vendor/**",
        "**/pycache"
    ],
    "files.exclude": {
        "**/pycache": true
    }
}